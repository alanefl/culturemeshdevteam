# Use PHP5.3 Single php.ini as default
AddHandler application/x-httpd-php53s .php

RewriteEngine on
RewriteCond %{HTTP_HOST} ^culturemesh\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.culturemesh\.com$
RewriteRule ^/?$ "http\:\/\/culturemesh\.launchrock\.com\/" [R=301,L]


RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://culturemesh.com/$1 [R=301,L]
