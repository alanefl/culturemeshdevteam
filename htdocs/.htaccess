# Use PHP54.4 as default
AddHandler application/x-httpd-php54 .php

# Enable Rewriting
RewriteEngine on

# Redirect to launchrock page
RewriteCond %{HTTP_HOST} ^culturemesh\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.culturemesh\.com$
RewriteRule ^/?$ "http\:\/\/culturemesh\.launchrock\.com\/" [R=302,L]

